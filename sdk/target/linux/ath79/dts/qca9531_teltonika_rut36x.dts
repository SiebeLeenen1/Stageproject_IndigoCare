/dts-v1/;

#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/input/input.h>
#include "qca953x.dtsi"
#include "platform_name.dtsi"

/ {
	compatible = "teltonika,rut36x", "qca,qca9531";
	model = "TELTONIKA RUT360";

	aliases {
		serial0 = &uart;
	};

	ext_io {
		compatible = "spi-gpio";
		#address-cells = <1>;
		#size-cells = <0>;

		gpio-sck = <&gpio 0 GPIO_ACTIVE_HIGH>;  // SRCLK
		gpio-mosi = <&gpio 2 GPIO_ACTIVE_HIGH>; // SER
		cs-gpios = <&gpio 1 GPIO_ACTIVE_HIGH>;  // RCLK
		num-chipselects = <1>;

		shift_io: shift_io@0 {
			compatible = "fairchild,74hc595";
			reg = <0>;
			gpio-controller;
			#gpio-cells = <2>;
			registers-number = <2>;
			spi-max-frequency = <10000000>;
		};
	};

	gpio-export {
		compatible = "gpio-export";
		#size-cells = <0>;

		gpio_modem_reset {
			gpio-export,name = "modem_reset";
			gpio-export,output = <0>;
			gpios = <&gpio 15 GPIO_ACTIVE_HIGH>;
		};

		gpio_modem_power {
			gpio-export,name = "modem_power";
			gpio-export,output = <0>;
			gpios = <&gpio 16 GPIO_ACTIVE_HIGH>;
		};
	};

	leds {
		compatible = "gpio-leds";

		eth1_led {
			label = "eth1_led";
			gpios = <&gpio 4 GPIO_ACTIVE_HIGH>;
		};

		eth2_led {
			label = "eth2_led";
			gpios = <&gpio 13 GPIO_ACTIVE_HIGH>;
		};

		led_gen_3 {
			label = "mob_gen_3";
			gpios = <&shift_io 1 GPIO_ACTIVE_HIGH>;
		};

		led_gen_4 {
			label = "mob_gen_4";
			gpios = <&shift_io 2 GPIO_ACTIVE_HIGH>;
		};

		led_ssid_1 {
			label = "mob_ssid_1";
			gpios = <&shift_io 3 GPIO_ACTIVE_HIGH>;
			linux,default-trigger = "timer";
		};

		led_ssid_3 {
			label = "mob_ssid_3";
			gpios = <&shift_io 4 GPIO_ACTIVE_HIGH>;
			linux,default-trigger = "timer";
		};

		led_ssid_5 {
			label = "mob_ssid_5";
			gpios = <&shift_io 5 GPIO_ACTIVE_HIGH>;
			linux,default-trigger = "timer";
		};

		led_power {
			label = "power";
			gpios = <&shift_io 0 GPIO_ACTIVE_LOW>;
			default-state = "on";
			retain-state-shutdown;
		};
	};

	keys {
		compatible = "gpio-keys";

		reset {
			label = "reset";
			linux,code = <KEY_RESTART>;
			gpios = <&gpio 17 GPIO_ACTIVE_LOW>;
		};
	};

	tlt_gpios { // Modify according to wiki page "DTS syntax with Libgpiod changes"
		gpiochip_info {
			hw_0 {
				hwver = <0>;
				gpiochip_count = <2>;

				gpiochip_0 = <32>; // Built-in
				gpiochip_1 = <16>; // Shift Register
			};
		};

		ioman {
			GPIO_11 {
				line_name = "GPIO_IN_1";
				active_low;
			};
			GPIO_12 {
				line_name = "GPIO_IN_2";
				active_low;
			};
			GPIO_38 {
				line_name = "GPIO_OUT_2";
			};
			GPIO_39 {
				line_name = "GPIO_OUT_1";
			};
		};

		ledman {
			GPIO_4 {
				line_name = "LED_ETH_GREEN_1";
			};
			GPIO_13 {
				line_name = "LED_ETH_GREEN_2";
			};
			GPIO_32 {
				line_name = "LED_POWER";
				active_low;
			};
			GPIO_33 {
				line_name = "LED_TECH_GEN_3";
			};
			GPIO_34 {
				line_name = "LED_TECH_GEN_4";
			};
			GPIO_35 {
				line_name = "LED_SSID_1";
			};
			GPIO_36 {
				line_name = "LED_SSID_3";
			};
			GPIO_37 {
				line_name = "LED_SSID_5";
			};
		};

		misc {
			GPIO_15 {
				line_name = "GPIO_MODEM_RESET_1";
			};
			GPIO_16 {
				line_name = "GPIO_MODEM_POWER_1";
			};
		};
	};

	ioman {
		layout@0 {
			io@0 { // 4pin #3
				label = "dio0";
				block_type = BLOCK_4PIN;
				block_pin = "3";
				default_direction = "out";
				gpio_in = "GPIO_IN_1";
				gpio_out = "GPIO_OUT_1";
				io_param = "g0";
			};
			io@1 { // 4pin #4
				label = "dio1";
				block_type = BLOCK_4PIN;
				block_pin = "4";
				default_direction = "in";
				gpio_in = "GPIO_IN_2";
				gpio_out = "GPIO_OUT_2";
				io_param = "g1";
			};
		};
	};
};

&mnfinfo {
	simpin1 {
		reg = <&config 0x70 0x10>;
		type = "digit";
		default = "";
	};

	sim_cfg {
		reg = <&config 0x110 0x20>;
		type = "simcfg";
		default = "0110000";
		log;
	};

	sim_count {
		default =  "1";
	};
};


&spi {
	status = "okay";
	num-cs = <2>;
	cs-gpios = <0>, <&gpio 14 0>;

	flash@0 {
		compatible = "jedec,spi-nor";
		reg = <0>;
		spi-max-frequency = <25000000>;

		partitions {
			compatible = "fixed-partitions";
			#address-cells = <1>;
			#size-cells = <1>;

			boot: partition@0 {
				label = "u-boot";
				reg = <0x000000 0x020000>;
				read-only;
			};

			config: partition@20000 {
				label = "config";
				reg = <0x020000 0x010000>;
				read-only;
			};

			art: partition@30000 {
				label = "art";
				reg = <0x030000 0x010000>;
				read-only;
			};

			partition@40000 {
				compatible = "denx,uimage";
				label = "firmware";
				reg = <0x040000 0xf30000>;
			};

			partition@f70000 {
				label = "event-log";
				reg = <0xf70000 0x090000>;
			};
		};
	};

	mt29f: flash@1 {
		compatible = "spinand,mt29f";
		reg = <1>;
		spi-max-frequency = <25000000>;

		partitions {
			compatible = "fixed-partitions";
			#address-cells = <1>;
			#size-cells = <1>;

			nand_ubi: partition@0 {
				label = "nand_ubi";
				reg = <0x000000 0x10000000>;
			};
		};
	};
};

&usb0 {
	#address-cells = <1>;
	#size-cells = <0>;
	status = "okay";

	hub_port0: port@1 {
		reg = <1>;
		#trigger-source-cells = <0>;
	};
};

&usb_phy {
	status = "okay";
};

&uart {
	status = "okay";
};

&eth1 {
	status = "okay";
	mtd-mac-address = <&config 0x0>;
};

&eth0 {
	status = "okay";
	phy-handle = <&swphy4>;
	mtd-mac-address = <&config 0x0>;
	mtd-mac-address-increment = <1>;
};

&wmac {
	status = "okay";
	mtd-cal-data = <&art 0x1000>;
	mtd-mac-address = <&config 0x0>;
	mtd-mac-address-increment = <2>;
};
